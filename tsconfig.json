{
    "compilerOptions": {
        "jsx": "react-jsx",
        "lib": [ "es2019", "dom" ],
        "module": "amd",
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": false,
        "inlineSourceMap": true,
        "target": "es5",
        "paths": {
            "react/jsx-runtime": [ "./js/lib/types/jsx-runtime" ],
            "react-dom": [ "./js/lib/types/react-dom" ],
            "react": [ "./js/lib/types/react" ]
        }
    },
    "include": ["js/**/*.tsx"]
}
